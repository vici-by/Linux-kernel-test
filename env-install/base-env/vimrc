""å®šä¹‰å‡½æ•°SetTitleï¼Œè‡ªåŠ¨æ’å…¥æ–‡ä»¶å¤´ 
func SetTitle() 
    "å¦‚æœæ–‡ä»¶ç±»å‹ä¸º.shæ–‡ä»¶ 
    if &filetype == 'sh' 
        call setline(1,"\#!/bin/bash") 
        call append(line("."), "\########################################################################")
        call append(line(".")+1, "\# File Name: ".expand("%")) 
        call append(line(".")+2, "\# Author: Baiy") 
        call append(line(".")+3, "\# mail: baiyang0223@163.com") 
        "call append(line(".")+4, "\# Created Time: ".strftime("%c")) 
        call append(line(".")+4, "\# Created Time: ".strftime("20%y-%m-%d-%H:%M:%S")) 
        call append(line(".")+5, "\# Func: ") 
        call append(line(".")+6, "\#########################################################################") 
        call append(line(".")+7, "") 
    elseif &filetype == 'py' 
        call setline(1,"\#!/usr/bin/python3") 
        call append(line("."), "\#-*- coding: UTF-8 -*-")
        call append(line(".")+1, "\########################################################################")
        call append(line(".")+2, "\# File Name: ".expand("%")) 
        call append(line(".")+3, "\# Author: Baiy") 
        call append(line(".")+4, "\# mail: baiyang0223@163.com") 
        "call append(line(".")+4, "\# Created Time: ".strftime("%c")) 
        call append(line(".")+5, "\# Created Time: ".strftime("20%y-%m-%d-%H:%M:%S")) 
        call append(line(".")+6, "\# Func: ") 
        call append(line(".")+7, "\#########################################################################") 
        call append(line(".")+8, "") 
    else 
        call setline(1, "/*************************************************************************") 
        call append(line("."), "    > File Name: ".expand("%")) 
        call append(line(".")+1, "    > Author: baiy") 
        call append(line(".")+2, "    > Mail: baiyang0223@163.com ") 
        "call append(line(".")+3, "    > Created Time: ".strftime("%c")) 
        call append(line(".")+3, "    > Created Time: ".strftime("20%y-%m-%d-%H:%M:%S")) 
        call append(line(".")+4, "    > Func: ") 
        call append(line(".")+5, " ************************************************************************/") 
        call append(line(".")+6, "")
    endif
    if &filetype == 'cpp'
        call append(line(".")+6, "#include<iostream>")
        call append(line(".")+7, "using namespace std;")
        call append(line(".")+8, "")
    endif
    if &filetype == 'c'
        call append(line(".")+7, "#include <stdio.h>")
        call append(line(".")+8, "")
    endif
    "æ–°å»ºæ–‡ä»¶åï¼Œè‡ªåŠ¨å®šä½åˆ°æ–‡ä»¶æœ«å°¾
    autocmd BufNewFile * normal G
endfunc 
